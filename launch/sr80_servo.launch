<launch>
  
  <!-- publish tf for calculating cartesian coordinate at web side -->
  <node name="tf2_web_republisher" pkg="tf2_web_republisher" type="tf2_web_republisher" output="screen"/>

  <!-- pilz motion programing needs speed override -->
  <include file="$(find prbt_hardware_support)/launch/fake_safety_interface.launch"/>

  <node name="servo_node" pkg="moveit_servo" type="servo_server" output="screen" >
    <param name="parameter_ns" type="string" value="optional_parameter_namespace" />
    <rosparam ns="optional_parameter_namespace" command="load" file="$(find sr80_moveit_config)/config/sr80_simulated_config.yaml" />
  </node>  

</launch>